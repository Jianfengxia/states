# {{ pillar['message_do_not_modify'] }}
[uwsgi]
master = true

rack = ../config.ru
chdir = /usr/local/graylog2-web-interface-{{ pillar['graylog2']['server']['version'] }}/script
module = sentry.wsgi
uid = www-data
gid = www-data
threaded-logger = true
#logger = graylog2:127.0.0.1:12201,uwsgi-graylog2
carbon-id = uwsgi-graylog2
carbon =127.0.0.1:2003
virtualenv = /usr/local/sentry
chdir = /usr/local/sentry
procname = sentry
procname-master = sentry-master
socket = /var/run/uwsgi/graylog2.sock
