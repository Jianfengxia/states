# {{ salt['pillar.get']('message_do_not_modify') }}

/var/log/graphite/carbon/*.log {
    daily
    missingok
    rotate 7
    compress
    delaycompress
    notifempty
    sharedscripts
    postrotate
        restart carbon > /dev/null
    endscript
}
