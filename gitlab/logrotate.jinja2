/home/gitlab/gitlabhq-{{ version }}/log/*.log {
    daily
    missingok
    rotate 90
    compress
    notifempty
    copytruncate
}

/var/log/gitlab/gitlab-shell/gitlab-shell.log {
    daily
    missingok
    rotate 90
    compress
    notifempty
    copytruncate
}
