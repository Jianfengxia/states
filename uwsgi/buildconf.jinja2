# {{ pillar['message_do_not_modify'] }}
[uwsgi]
xml = false
ini = true
yaml = false
json = false
sqlite3 = false
zeromq = false
snmp = false
spooler = true
embedded = true
ssl = false
udp = true
multicast = false
threading = true
minterpreters = true
async = true
ldap = false
pcre = false
routing = false
alarm = false
ipv6 = false
debug = false
unbit = false
graylog2 = true
{% if salt['pkg.version']('ruby1.9.1-dev') %}
rack = true
{% endif %}
{% if salt['cmd.has_exec']('php-config') %}
php = true
{% endif %}
{% if 'graylog2_address' not in pillar %}
syslog = true
{% endif %}
xml_implementation = libxml2
yaml_implementation = auto
malloc_implementation = libc
extras =
plugins =
bin_name = uwsgi
append_version =
plugin_dir = .
embedded_plugins = {% if salt['pkg.version']('ruby1.9.1-dev') %}rack, {% endif %}{% if salt['cmd.has_exec']('php-config') %}php, {% endif %}python, gevent, ping, nagios, carbon, signal, {% if 'graylog2_address' in pillar %}graylog2{% else %}syslog{% endif %}
as_shared_library = false

locking = auto
event = auto
timer = auto
filemonitor = auto

blacklist =
whitelist =

embed_files =

embed_config =
